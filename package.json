{
  "name": "dsek-web",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "prepare": "husky install && svelte-kit sync && zenstack generate && prisma generate && pnpm paraglide && pnpm snaplet-seed generate",
    "setup-dev": "vite-node dev/setup.ts",
    "ci-check": "concurrently 'pnpm:lint' 'pnpm:format' 'pnpm:test' 'pnpm:check' --prefix-colors 'auto' --group",
    "dev": "vite dev",
    "build": "vite build",
    "preview": "pm2 start prod/ecosystem.config.cjs --no-daemon",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier . --check",
    "format:fix": "prettier . --write",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test": "pnpm test:unit",
    "test:integration": "playwright test",
    "test:unit": "vitest run --retry 2",
    "generate": "zenstack generate && prisma generate",
    "migrate": "zenstack generate && prisma migrate dev && snaplet-seed sync",
    "seed": "prisma db seed",
    "prisma-case-format": "prisma-case-format -f src/database/prisma/schema.prisma",
    "paraglide": "paraglide-js compile --project ./project.inlang --outdir ./src/translations/paraglide",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "zenstack": {
    "schema": "src/database/schema.zmodel"
  },
  "prisma": {
    "schema": "src/database/prisma/schema.prisma",
    "seed": "vite-node src/database/seed/main.ts"
  },
  "@snaplet/seed": {
    "config": "src/database/seed/seed.config.ts"
  },
  "devDependencies": {
    "@clack/prompts": "^0.7.0",
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.15.0",
    "@event-calendar/core": "^3.6.2",
    "@event-calendar/resource-timeline": "^3.6.2",
    "@faker-js/faker": "^9.2.0",
    "@iconify-json/flag": "^1.2.2",
    "@iconify-json/mdi": "^1.2.1",
    "@inlang/paraglide-js": "1.11.3",
    "@inlang/paraglide-js-adapter-sveltekit": "^0.6.7",
    "@snaplet/copycat": "^5.1.0",
    "@snaplet/seed": "0.98.0",
    "@sveltejs/adapter-node": "^5.2.9",
    "@sveltejs/adapter-vercel": "^5.4.6",
    "@sveltejs/kit": "^2.7.5",
    "@sveltejs/vite-plugin-svelte": "^4.0.0",
    "@tailwindcss/typography": "^0.5.15",
    "@types/eslint": "^9.6.1",
    "@types/node": "^22.9.0",
    "@types/node-schedule": "^2.1.7",
    "@types/nodemailer": "^6.4.16",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.13.0",
    "@typescript-eslint/parser": "^8.13.0",
    "@whiterussianstudio/tailwind-easing": "^1.0.0",
    "@zenstackhq/openapi": "^2.7.5",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.1.0",
    "daisyui": "^4.12.14",
    "eslint": "^9.14.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-svelte": "^2.46.0",
    "globals": "^15.12.0",
    "husky": "^9.1.6",
    "postcss": "^8.4.47",
    "postcss-import": "^16.1.0",
    "prettier": "^3.3.3",
    "prettier-plugin-svelte": "^3.2.7",
    "prettier-plugin-tailwindcss": "^0.6.8",
    "prisma": "^5.22.0",
    "prisma-case-format": "^2.2.1",
    "svelte": "^5.1.9",
    "svelte-check": "^4.0.5",
    "sveltekit-flash-message": "^2.4.4",
    "sveltekit-superforms": "^2.20.0",
    "tailwindcss": "^3.4.14",
    "tslib": "^2.8.1",
    "typescript": "^5.6.3",
    "vite": "^5.4.10",
    "vite-node": "^2.1.4",
    "vitepress": "^1.5.0",
    "vitest": "^2.1.4",
    "vitest-mock-extended": "^2.0.2",
    "zenstack": "^2.7.5",
    "zod": "^3.23.8"
  },
  "type": "module",
  "dependencies": {
    "@auth/core": "^0.37.3",
    "@auth/sveltekit": "^1.7.3",
    "@egoist/tailwindcss-icons": "^1.8.1",
    "@fontsource-variable/inter": "^5.1.0",
    "@fontsource/ibm-plex-mono": "^5.1.0",
    "@fontsource/lexend": "^5.1.1",
    "@fullcalendar/core": "^6.1.15",
    "@fullcalendar/daygrid": "^6.1.15",
    "@keycloak/keycloak-admin-client": "^26.0.5",
    "@playwright/test": "^1.48.2",
    "@prisma/client": "^5.22.0",
    "@shlinkio/shlink-js-sdk": "^1.2.0",
    "@stripe/stripe-js": "^4.9.0",
    "@tiptap/core": "^2.9.1",
    "@tiptap/pm": "^2.9.1",
    "@tiptap/starter-kit": "^2.9.1",
    "@vercel/speed-insights": "^1.0.14",
    "@zenstackhq/runtime": "^2.7.5",
    "@zenstackhq/server": "^2.7.5",
    "croppie": "^2.6.5",
    "dayjs": "^1.11.13",
    "embla-carousel-autoplay": "^8.3.1",
    "embla-carousel-svelte": "^8.3.1",
    "expo-server-sdk": "^3.11.0",
    "express": "^4.21.1",
    "express-queue": "^0.0.13",
    "ics": "^3.8.1",
    "isomorphic-dompurify": "^2.16.0",
    "markdown-to-txt": "^2.0.1",
    "marked": "^14.1.3",
    "minio": "^8.0.2",
    "node-schedule": "^2.1.1",
    "nodemailer": "^6.9.16",
    "sharp": "^0.33.5",
    "stripe": "^17.3.1",
    "svelte-easy-crop": "^3.0.1",
    "svelte-stripe": "^1.3.0",
    "tailwind-merge": "^2.5.4",
    "tiptap-markdown": "^0.8.10",
    "uuid": "^11.0.2",
    "vite-plugin-cjs-interop": "^2.1.5"
  },
  "engines": {
    "node": "<=23"
  },
  "packageManager": "pnpm@9.12.2",
  "pnpm": {
    "patchedDependencies": {
      "devalue@4.3.2": "patches/devalue@4.3.2.patch",
      "devalue@5.1.1": "patches/devalue@5.1.1.patch"
    }
  }
}
