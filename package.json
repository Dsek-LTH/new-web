{
  "name": "dsek-web",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "prepare": "husky install && svelte-kit sync && zenstack generate && prisma generate && pnpm paraglide",
    "setup-dev": "vite-node dev/setup.ts",
    "ci-check": "concurrently 'pnpm:lint' 'pnpm:format' 'pnpm:test' 'pnpm:check' --prefix-colors 'auto' --group",
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier . --check",
    "format:fix": "prettier . --write",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "generate": "zenstack generate && prisma generate",
    "migrate": "zenstack generate && prisma migrate dev",
    "seed": "prisma db seed",
    "prisma-case-format": "prisma-case-format -f src/database/prisma/schema.prisma",
    "paraglide": "paraglide-js compile --project ./project.inlang --outdir ./src/translations/paraglide"
  },
  "prisma": {
    "schema": "src/database/prisma/schema.prisma",
    "seed": "vite-node src/database/seed/main.ts"
  },
  "zenstack": {
    "schema": "src/database/schema.zmodel"
  },
  "devDependencies": {
    "@clack/prompts": "^0.7.0",
    "@iconify-json/mdi": "^1.1.63",
    "@inlang/paraglide-js": "1.1.0",
    "@inlang/paraglide-js-adapter-sveltekit": "^0.4.1",
    "@sveltejs/adapter-node": "^2.1.2",
    "@sveltejs/adapter-vercel": "^4.0.3",
    "@sveltejs/kit": "^2.4.3",
    "@sveltejs/vite-plugin-svelte": "^3.0.1",
    "@tailwindcss/typography": "^0.5.10",
    "@types/eslint": "^8.56.2",
    "@types/node": "^20.10.5",
    "@types/node-schedule": "^2.1.6",
    "@types/nodemailer": "^6.4.14",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "@zenstackhq/openapi": "^1.12.3",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.2",
    "daisyui": "^4.4.24",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-svelte": "^2.35.1",
    "husky": "^8.0.3",
    "postcss": "^8.4.32",
    "prettier": "^3.1.1",
    "prettier-plugin-svelte": "^3.1.2",
    "prettier-plugin-tailwindcss": "^0.5.9",
    "prisma-case-format": "^2.2.0",
    "svelte": "^4.2.8",
    "svelte-check": "^3.6.9",
    "sveltekit-flash-message": "^2.3.0",
    "sveltekit-superforms": "^1.13.1",
    "tailwindcss": "^3.4.0",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3",
    "vite": "^5.0.13",
    "vite-node": "^1.1.0",
    "vitest": "^1.2.2",
    "vitest-mock-extended": "^1.3.1",
    "zenstack": "^1.12.3",
    "zod": "^3.22.4"
  },
  "type": "module",
  "dependencies": {
    "@auth/core": "^0.19.0",
    "@auth/sveltekit": "^0.5.0",
    "@egoist/tailwindcss-icons": "^1.7.1",
    "@fullcalendar/core": "^6.1.10",
    "@fullcalendar/daygrid": "^6.1.10",
    "@keycloak/keycloak-admin-client": "^23.0.6",
    "@prisma/client": "^5.12.1",
    "@stripe/stripe-js": "^3.3.0",
    "@tiptap/core": "^2.1.13",
    "@tiptap/pm": "^2.1.13",
    "@tiptap/starter-kit": "^2.1.13",
    "@vercel/speed-insights": "^1.0.10",
    "@zenstackhq/runtime": "^1.12.3",
    "@zenstackhq/server": "^1.12.3",
    "croppie": "^2.6.5",
    "dayjs": "^1.11.10",
    "embla-carousel-autoplay": "^8.0.2",
    "embla-carousel-svelte": "^8.0.2",
    "expo-server-sdk": "^3.7.0",
    "isomorphic-dompurify": "^2.0.0",
    "markdown-to-txt": "^2.0.1",
    "marked": "^11.1.0",
    "minio": "^7.1.3",
    "node-schedule": "^2.1.1",
    "nodemailer": "^6.9.9",
    "prisma": "^5.12.1",
    "sharp": "^0.33.1",
    "stripe": "^14.25.0",
    "svelte-easy-crop": "^2.0.2",
    "svelte-stripe": "^1.1.7",
    "tailwind-merge": "^2.2.0",
    "tiptap-markdown": "^0.8.8",
    "uuid": "^9.0.1",
    "vite-plugin-cjs-interop": "^2.1.0"
  },
  "pnpm": {
    "patchedDependencies": {
      "devalue@4.3.2": "patches/devalue@4.3.2.patch"
    }
  },
  "engines": {
    "node": "<=22"
  }
}
